{
  "manifest_version": 3,
  "name": "YClick",
  "description": "Automatically click YouTube \"Skip Ads\" Button",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["scripts/content.js"]
    }
  ]
}
